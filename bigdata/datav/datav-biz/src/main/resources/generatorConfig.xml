<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="my" targetRuntime="MyBatis3">
        <!-- 格式化JAVA代码 -->
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
        <!-- 格式化XML代码 -->
        <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>

        <property name="autoDelimitKeywords" value="true" />
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>


        <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>

        <!--
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin">
        -->

        <plugin type="com.softwareloop.mybatis.generator.plugins.LombokPlugin">

            <!-- enable annotations -->
            <property name="builder" value="false"/>
            <!-- annotation's option(java.lang.Boolean) -->
            <property name="builder.fluent" value="false"/>
            <!-- annotation's option(String) -->
            <property name="builder.builderMethodName" value="Builder"/>

            <property name="accessors" value="false"/>
            <!-- annotation's option(array of String) -->
            <property name="accessors.prefix" value="m_, _"/>

            <!-- disable annotations -->
            <property name="allArgsConstructor" value="false"/>
        </plugin>

        <commentGenerator>
            <property name="suppressDate" value="false"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/wolf_datav?useSSL=false"
                        userId="root"
                        password="12345678">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="true"/>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="study.daydayup.wolf.bigdata.datav.biz.dal.dataobject"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mapper/auto"
                         targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="study.daydayup.wolf.bigdata.datav.biz.dal.dao"
                             targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="beat" domainObjectName="BeatDO" mapperName="BeatDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
        </table>

        <table tableName="daily_koi" domainObjectName="DailyKoiDO" mapperName="DailyKoiDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_sms" domainObjectName="DailySmsDO" mapperName="DailySmsDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_trade" domainObjectName="DailyTradeDO" mapperName="DailyTradeDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_loan" domainObjectName="DailyLoanDO" mapperName="DailyLoanDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_repay" domainObjectName="DailyRepayDO" mapperName="DailyRepayDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_collection" domainObjectName="DailyCollectionDO" mapperName="DailyCollectionDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="daily_collector" domainObjectName="DailyCollectorDO" mapperName="DailyCollectorDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="model_collector" domainObjectName="ModelCollectorDO" mapperName="ModelCollectorDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="track_repay" domainObjectName="TrackRepayDO" mapperName="TrackRepayDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>

        <table tableName="offset_holder" domainObjectName="OffsetHolderDO" mapperName="OffsetHolderDAO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByPrimaryKey="false" enableDeleteByExample="false" enableSelectByExample="false" >
            <columnOverride column="locker" javaType="java.lang.Integer" jdbcType="TINYINT" />
            <columnOverride column="delete_flag" javaType="java.lang.Boolean"/>
        </table>



    </context>
</generatorConfiguration>